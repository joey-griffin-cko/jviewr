    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch api",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/src/api/bin/Debug/netcoreapp2.2/api.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/api",
            "stopAtEntry": false,
            "launchBrowser": {
                "enabled": false
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ASPNETCORE_URLS": "http://*:9341"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        }
    ]
}